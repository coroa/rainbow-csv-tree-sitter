{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "csv",
      "camelcase": "Csv",
      "title": "Rainbow CSV",
      "scope": "source.csv",
      "file-types": ["csv"],
      "injection-regex": "^csv$",
      "class-name": "TreeSitterCsv",
      "path": "csv"
    },
    {
      "name": "ssv",
      "camelcase": "Ssv",
      "title": "Rainbow CSV (semicolon)",
      "scope": "source.csv",
      "file-types": ["csv"],
      "class-name": "TreeSitterSvs",
      "path": "ssv"
    },
    {
      "name": "psv",
      "camelcase": "Psv",
      "title": "Rainbow CSV (pipes)",
      "scope": "source.psv",
      "file-types": ["psv"],
      "class-name": "TreeSitterPsv",
      "path": "psv"
    },
    {
      "name": "tsv",
      "camelcase": "Tsv",
      "title": "Rainbow CSV (tabs)",
      "scope": "source.tsv",
      "file-types": ["psv"],
      "class-name": "TreeSitterTsv",
      "path": "tsv"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "ISC",
    "description": "Rainbow Csv grammar for tree-sitter in zed",
    "authors": [{ "name": "Hans" }, { "name": "coroa" }],
    "links": {
      "repository": "https://github.com/coroa/rainbow-csv-tree-sitter"
    }
  },
  "bindings": {
    "c": false,
    "go": false,
    "node": false,
    "python": false,
    "rust": true,
    "swift": false,
    "zig": false
  }
}
